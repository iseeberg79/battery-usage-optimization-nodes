            "EvaluateSolarForecast": "nodes/evaluateSolarForecast.js",
            "EvaluateSolarForecastAPI": "nodes/evaluateSolarForecastAPI.js",
            "EvaluateSolarForecastOpenMeteo": "nodes/evaluateSolarForecastOpenMeteo.js",
            "ControlBattery": "nodes/controlBattery.js",
            "DeterminePowerValues": "nodes/determinePowerValues.js",
            "DetermineControlMode": "nodes/determineControlMode.js",
            "EstimateHouseholdConsumption": "nodes/estimateHouseholdConsumption.js",
            "EvaluateGridEnergyPricesAPI": "nodes/evaluateGridEnergyPricesAPI.js",
            "EvaluateGridEnergyPrices": "nodes/evaluateGridEnergyPrices.js",
            "EstimateSolarEnergy": "nodes/estimateSolarEnergy.js",
            "EstimateBatterymode": "nodes/estimateBatterymode.js",
            "BatteryModeControl": "nodes/batteryModeControl.js",
            "TibberApiPrices": "nodes/tibberApiPrices.js",
            "EvoptBatteryMode": "nodes/evoptBatteryMode.js",
            "PrepareForecastData": "nodes/prepareForecastData.js",
            "CombinePVForecasts": "nodes/combinePVForecasts.js"
        }
