{
    "name": "@iseeberg79/battery-usage-optimization-nodes",
    "version": "0.2.0-21",
    "description": "A custom Node-RED package for battery usage optimization",
    "main": "ControlBattery.js",
    "scripts": {
        "grunt": "grunt",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:verbose": "jest --verbose",
        "test:unit": "jest test/.*\\.test\\.js --testPathIgnorePatterns=integration",
        "test:integration": "jest test/integration",
        "lint": "eslint nodes/**/*.js",
        "lint:fix": "eslint nodes/**/*.js --fix"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/iseeberg79/battery-usage-optimization-nodes.git"
    },
    "bugs": {
        "url": "https://github.com/iseeberg79/battery-usage-optimization-nodes/issues"
    },
    "homepage": "https://github.com/iseeberg79/battery-usage-optimization-nodes#readme",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "node-red",
        "battery-optimization",
        "solar-energy-control"
    ],
    "author": "Ingo Seeberg-Hinrichs",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.0",
        "node-red": "^2.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "eslint": "^8.57.0",
        "grunt": "~0.4.5",
        "grunt-cli": "^0.1.13",
        "grunt-contrib-nodeunit": "~0.4.1",
        "grunt-eslint": "^19.0.0",
        "grunt-jsdoc": "~2.2.0",
        "grunt-shell": "~1.3.0",
        "jest": "^29.7.0",
        "jsdoc": "^4.0.5"
    },
    "node-red": {
        "version": ">=2.0.0",
        "nodes": {
